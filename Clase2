--Vector

entity ejemplo21 is
port (A,B, : in bit_vector (3 downto 0);  
C: inout bit_vector(3 downto 0); 
S: out bit_vector (3 downto 0));
end ejemplo21;




--Ejercicio de Vector
entity ejemplos is
port (Q: in bit_vector (3 downto 0); 
F: out bit_vector (0 to 7));
end ejemplos;




--Ejercicio de Vector
library ieee;
